#create a new repo on the command line
echo "# In_Github_Actions" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/pavanchandra2494/In_Github_Actions.git
git push -u origin main
#pushing an exixting repo from command line
git remote add origin https://github.com/pavanchandra2494/In_Github_Actions.git
git branch -M main
git push -u origin main

